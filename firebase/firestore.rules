rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read: if false;
      allow write: if false;
    }
    match /most-popular/{document=**} {
      allow read: if true;
    }
    match /top-picks/{document=**} {
      allow read: if true;
    }
    match /india/{document=**} {
    	allow read: if true;
    }
    match /IN/{document=**} {
    	allow read: if true;
    }
    match /US/{document=**} {
    	allow read: if true;
    }
    match /CA/{document=**} {
    	allow read: if true;
    }
    match /GB/{document=**} {
    	allow read: if true;
    }
  }
}